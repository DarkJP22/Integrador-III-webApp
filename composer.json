{
    "name": "gpsmedica/gpsmedica",
    "description": "Doctor Blue",
    "keywords": ["GPS", "Medica"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.4",
        "barryvdh/laravel-dompdf": "^v3.1.1",
        "cloudinary-labs/cloudinary-laravel": "^3.0",
        "guzzlehttp/guzzle": "^7.9.3",
        "laravel/framework": "^12.5",
        "laravel/telescope": "^5.7.0",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^4.6.1",
        "league/flysystem-aws-s3-v3": "3.0",
        "maatwebsite/excel": ">=3.1.64",
        "pda/pheanstalk": "^4.0.5",
        "pusher/pusher-php-server": "^7.2.7",
        "spatie/laravel-backup": "^9.2.9",
        "twilio/sdk": "^7.16.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15.2",
        "fakerphp/faker": "^1.24.1",
        "laravel/pint": "^1.21.2",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.7.0",
        "pestphp/pest": "^3.8.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "phpunit/phpunit": "^11.5.15",
        "spatie/laravel-ignition": "^2.9.1"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#c4b5fd,#fb7185,#fdba74\" \"php artisan queue:listen --queue=billing,default --tries=1\" \"npm run tailwind\" \"npm run vite:dev\" --names=queue,tailwind,vite"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
